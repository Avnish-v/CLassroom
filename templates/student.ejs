	<body class="bg-dark">
		<p
			style="
				display: inline-block;
				padding: 7px 9px;
				color: white;
				margin-top: 12px;
				margin-left: 12px;
				font-weight: bolder;
				font-size: large;
				border: 2px solid white;
				border-radius: 20px;
			"
		>
			<i class="fa fa-user" aria-hidden="true"> <%= myVar %> </i>
		</p>

		<h1 class="text-light d-flex justify-content-center">Student</h1>
